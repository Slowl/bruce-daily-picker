{
  "short_name": "Google Meet Picker",
  "name": "Google Meet Picker",
  "description": "Don't uninstall me or I'll eat you :wink:",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Pick someone !"
  },
  "content_scripts": [
    {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["./static/js/content.js"]
    }
 ],
  "icons": {
    "16": "/img/dice_128.png",
    "48": "/img/dice_128.png",
    "128": "/img/dice_128.png"
  },
  "permissions": [
    "activeTab"
 ]
}
